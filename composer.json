{
    "name": "Soapbox/customckeditor",
    "description": "An extension to allow for CKeditor customisations.",
    "type": "bolt-extension",
    "keywords": [
        "foo",
        "bar"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Soapbox",
            "email": "support@soapbox.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\CustomCkeditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\CustomCkeditor\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Soapbox\\CustomCkeditor\\CustomCkeditorExtension"
    }
}
